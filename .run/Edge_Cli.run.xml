<?xml version="1.0" encoding="UTF-8"?>
<component name="ProjectRunConfigurationManager">
    <configuration default="false" name="Edge_Cli" type="Application" factoryName="Application">
        <option name="MAIN_CLASS_NAME" value="org.apache.iotdb.cli.Cli"/>
        <module name="iotdb-cli"/>
        <option name="PROGRAM_PARAMETERS" value="-h 127.0.0.1 -p 6668 -u root -pw root"/>
        <option name="VM_PARAMETERS" value="-DCONFIGNODE_CONF=./conf_edge -Dlogback.configureationFile=./conf_edge/logback-confignode.xml -DTSFILE=./conf_edge -Dname=iotdb/.ConfigNodeEdge -DIOTDB_CONF=./conf_edge"/>
        <extension name="coverage">
            <pattern>
                <option name="PATTERN" value="org.apache.iotdb.cli.*"/>
                <option name="ENABLED" value="true"/>
            </pattern>
        </extension>
        <method v="2">
            <option name="Make" enabled="true"/>
        </method>
    </configuration>
</component>
